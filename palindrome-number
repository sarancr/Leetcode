https://leetcode.com/problems/palindrome-number/

var isPalindrome = function(x) {
    
    
    if(x <0) 
        return false;
    
    let a = x;
    
    let reversed = 0;
    
    while(x !== 0){
        reversed =  (reversed * 10) + Math.floor(x %10);
        
        x = Math.floor(x / 10);
    

    }
    
    console.log(reversed);
              return a === reversed ? true : false;

    
};
